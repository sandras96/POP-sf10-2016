<Window x:Class="POP_SF_9_2016.UI.NamestajWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_9_2016.UI"
        mc:Ignorable="d"
        Title="NamestajWindow" Height="300" Width="300">
    <Grid>
        <Label Grid.Row="0" Grid.Column="0" Content="Naziv" HorizontalAlignment="Left" Width="30" FontSize="15"/>
        <TextBox Grid.Row="0" Grid.Column="1" FontSize="15" HorizontalAlignment="Left" Name="tbNaziv"/>
        <Button Content="Sacuvaj" HorizontalAlignment="Right" Click="SacuvajIzmene"/>
        <Button Content="Izlaz"HorizontalAlignment="Right"/>
    </Grid>
</Window>
